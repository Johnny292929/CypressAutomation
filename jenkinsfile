stage('run test') {
  parallel {
   stage('tester A') {
    steps {
     bat "npm run test"
    }
   }
   // second tester runs the same command
   stage('tester B') {
    steps {
    bat "npm run test"
    }
   }
   // third tester runs the same command
   stage('tester C') {
    steps {
    bat "npm run test"
    }
   }
   // fourth tester runs the same command
   stage('tester D') {
    steps {
     bat "npm run test"
    }
   }
  }