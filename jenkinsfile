pipeline{
    agent any
   // tools{nodejs "node"}
stages{

    stage('cypress parallel tests'){

        parallel{
            stage('Agent 1'){

                steps{
                    bat "npm run test"
                }
            }
             stage('Agent 2'){

                steps{
                    bat "npm run test"
                }
             }Parallel 1
             
             stage('Agent 3'){

                steps{
                    bat "npm run test"
                }
             }
             
               stage('Agent 4){

                steps{
                    bat "npm run test"
                }
             }

        }

    }

}

}
