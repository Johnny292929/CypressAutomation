    stage('cypress parallel tests'){

        parallel{
            stage('tester a'){

                steps{
                    bat "npm run cy:run"
                }
            }
             stage('tester b'){

                steps{
                    bat "npm run cy:run"
                }
             }

        }

    }

}

}
